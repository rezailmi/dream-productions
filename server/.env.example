# WHOOP OAuth Configuration
WHOOP_CLIENT_ID=your_whoop_client_id_here
WHOOP_CLIENT_SECRET=your_whoop_client_secret_here
WHOOP_CALLBACK_URL=http://localhost:3000/auth/whoop/callback

# OpenAI API Configuration (for dream narrative generation)
OPENAI_API_KEY=your_openai_api_key_here

# Video Generation Provider (google or fal)
VIDEO_PROVIDER=google

# Google Veo Configuration (for direct Google Veo video generation)
GOOGLE_VEO_API_KEY=your_google_veo_api_key_here

# Fal.ai Configuration (alternative provider for Veo3 video generation)
FAL_API_KEY=your_fal_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development

# Session Secret (generate a random string for production)
SESSION_SECRET=your_session_secret_here

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:8081
