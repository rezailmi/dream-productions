{
  "expo": {
    "name": "Dream Machine",
    "slug": "dream-machine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "dreammachine",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1625"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dreammachine.app",
      "deploymentTarget": "26.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSHealthShareUsageDescription": "Dream Machine needs access to your sleep data to reconstruct your dreams.",
        "NSHealthUpdateUsageDescription": "Dream Machine needs to update your health data."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1625"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.dreammachine.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ]
  }
}
